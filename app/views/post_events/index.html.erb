<h1>Listing post_events</h1>

<table>
  <thead>
    <tr>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @post_events.each do |post_event| %>
      <tr>
        <td><%= link_to 'Show', post_event %></td>
        <td><%= link_to 'Edit', edit_post_event_path(post_event) %></td>

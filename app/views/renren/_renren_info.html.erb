<h1> <%= @renren_user["name"] %> </h1>
<h2> id: <%=@renren_userId  %>  <%= image_tag(@renren_user["avatar"][3]["url"], alt: "Photo") %></h2>
<h3> 用户基本数据 </h3>
<p> 性别: <%= @renren_basic_info["basicInformation"]["sex"] %> </p>
<p> 生日: <%= @renren_basic_info["basicInformation"]["birthday"] %> </p>
<p> 家乡: <%= @renren_basic_info["basicInformation"]["homeTown"] %> </p>
<p> 学校信息:  </p>
	<%= schoolNum = @renren_basic_info["education"].count %>
	<% i = 0 %>
	<% while i < schoolNum do %>
	   <p> <%= @renren_basic_info["education"][i] %> </p>
	   <p> <%= @renren_basic_info["education"][i] %> </p>
	   <p> <%= @renren_basic_info["education"][i] %> </p>
	   <p> <%= @renren_basic_info["education"][i] %> </p>
	   <% i = i + 1 %>
	<% end %>
